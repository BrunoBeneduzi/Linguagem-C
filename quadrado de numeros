# Linguagem-C
Projetos iniciais
#include <stdio.h>
#include <stdlib.h>

int main(){
int n, a, b, c;

scanf("%d", &n);

for(a = 1; a <= (2 * n) - 1; a ++){
   c = n;
    for(b = 1; b <= (2 * n) - 1; b++){
      printf("%d ", c);
       if(b < a){
        c--;
       }
       if(b > (2 * n) - 1 - a){
        c++;
       }
    }
 printf("\n");
}


return 0;
}
